{
  "expo": {
    "owner":"bburger73",
    "scheme": "nativeapp",
    "name": "docker-project",
    "slug": "docker-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/notificationsound.wav",
            "./assets/notificationsoundother.wav"
          ],
          "mode": "production"
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.burgertechnologies.nativeapp",
      "buildNumber": "0.0.1",
      "infoPlist": {
        "NSUserActivityTypes": [
          "INSendMessageIntent"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.burgertechnologies.nativeapp",
      "versionCode":1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  },
  "name": "docker-project"
}
